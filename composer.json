{
    "name": "diginfinity/laravay",
    "type": "library",
    "description": "Another powerful ACL package for the Laravel Framework",
    "keywords": [
        "DigInfinity",
        "Laravay"
    ],
    "homepage": "https://github.com/diginfinity/laravay",
    "license": "MIT",
    "authors": [
        {
            "name": "Digital Infinity",
            "email": "gorankrgovic1@gmail.com",
            "homepage": "https://github.com/diginfinity",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1.3",
        "illuminate/auth": "~5.2",
        "illuminate/cache": "~5.2",
        "illuminate/console": "~5.2",
        "illuminate/database": "^5.2.32",
        "illuminate/support": "~5.2",
        "kkszymanowski/traitor": "^0.2.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3",
        "mockery/mockery": ">=0.9.9",
        "orchestra/testbench": "~3.2"
    },
    "autoload": {
        "psr-4": {
            "Laravay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravay\\LaravayServiceProvider"
            ],
            "aliases": {
                "Laravay": "Laravay\\LaravayFacade"
            }
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "sort-packages": true
    }
}
